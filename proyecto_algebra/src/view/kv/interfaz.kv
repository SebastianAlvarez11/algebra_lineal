<MiInterfaz>

    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 10

        Label:
            text: "Inventario"
            font_size: 24
            size_hint: 1, 0.1

        ScrollView:
            size_hint: 1, 0.8
            GridLayout:
                cols: 2
                size_hint_y: None
                height: self.minimum_height
            
                Label:
                    text: "Producto"
                    bold: True
                Label:
                    text: "Cantidad"
                    bold: True

               


